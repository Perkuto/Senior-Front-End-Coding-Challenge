{
  "name": "flickr-pictures",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start-dev": "concurrently \"npm run start-server\" \"npm run start-client\"",
    "start-prod": "ng serve --prod",
    "start-client": "ng serve",
    "start-server": "node server/server.js",
    "build": "ng build",
    "build-prod": "ng build --prod --output-path dist --base-href 'https://FaniloRandria.github.io/Senior-Front-End-Coding-Challenge/'",
    "test": "ng test --no-watch",
    "test-ci": "ng test --no-watch --code-coverage",
    "lint": "ng lint",
    "e2e": "concurrently -k -s first \"npm run start-server\" \"ng e2e --port 4300\"",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.0",
    "@angular/cdk": "6.4.2",
    "@angular/common": "6.1.0",
    "@angular/compiler": "6.1.0",
    "@angular/core": "6.1.0",
    "@angular/flex-layout": "6.0.0-beta.17",
    "@angular/forms": "6.1.0",
    "@angular/http": "6.1.0",
    "@angular/material": "6.4.2",
    "@angular/platform-browser": "6.1.0",
    "@angular/platform-browser-dynamic": "6.1.0",
    "@angular/router": "6.1.0",
    "@fortawesome/angular-fontawesome": "0.1.1",
    "@fortawesome/fontawesome-svg-core": "1.2.2",
    "@fortawesome/free-brands-svg-icons": "5.2.0",
    "@fortawesome/free-solid-svg-icons": "5.2.0",
    "@ngx-share/button": "6.0.1",
    "@ngx-share/core": "6.0.1",
    "concurrently": "3.6.1",
    "core-js": "2.5.7",
    "ngx-infinite-scroll": "6.0.1",
    "rxjs": "6.2.2",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.7.1",
    "@angular/cli": "6.1.1",
    "@angular/compiler-cli": "6.1.0",
    "@angular/language-service": "6.1.0",
    "@types/jasmine": "2.8.8",
    "@types/jasminewd2": "2.0.3",
    "@types/node": "10.5.4",
    "codelyzer": "4.4.2",
    "coveralls": "3.0.2",
    "jasmine-core": "3.1.0",
    "jasmine-spec-reporter": "4.2.1",
    "json-server": "0.14.0",
    "karma": "2.0.5",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage-istanbul-reporter": "2.0.1",
    "karma-jasmine": "1.1.2",
    "karma-jasmine-html-reporter": "1.2.0",
    "protractor": "5.4.0",
    "ts-node": "7.0.0",
    "tslint": "5.11.0",
    "typescript": "2.7.2"
  }
}
